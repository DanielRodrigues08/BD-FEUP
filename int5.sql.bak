/*
SELECT name, max(avgPace)
FROM (
SELECT idCar as idCarAux, round(avg(pace),2) as avgPace
FROM Setup, CarCircuitSetup
WHERE CarCircuitSetup.idSetup = Setup.idSetup
GROUP BY CarCircuitSetup.idCar), Pilot, Staff
WHERE idCarAux = Pilot.idCar AND Pilot.idStaff = Staff.idStaff;*/

SELECT idStaff, pace
FROM Pilot, Setup, CarCircuitSetup, Car
WHERE Car.idCar = Pilot.idCar AND Car.idCar = CarCircuitSetup.idCar AND Setup.idSetup = CarCircuitSetup.idSetup
GROUP BY idStaff, pace;